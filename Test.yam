@lang(u8) const u8 = u8;
@lang(u16) const u16 = u16;
@lang(u32) const u32 = u32;
@lang(u64) const u64 = u64;
@lang(usize) const usize = usize;

@lang(i8) const i8 = i8;
@lang(i16) const i16 = i16;
@lang(i32) const i32 = i32;
@lang(i64) const i64 = i64;
@lang(isize) const isize = isize;

@lang(f32) const f32 = f32;
@lang(f64) const f64 = f64;

@lang(bool) const bool = bool;

@lang(void) const void = void;

extern "C" fn puts(out: *const u8) -> i32;

fn a() { puts("a called"); }

fn b() { puts("b called"); }

fn main(argc: i32, argv: *const *const u8) -> i32 {
    var i = 0;
    while i < 10 {
        if i % 2 == 0 {
            a();
        } else {
            b();
        }

        i = i + 1;
    }

    0
}
